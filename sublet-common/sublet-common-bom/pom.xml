            <dependency>
                <groupId>com.wrh</groupId>
                <artifactId>sublet-common-feign</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.wrh</groupId>
                <artifactId>sublet-common-log</artifactId>
