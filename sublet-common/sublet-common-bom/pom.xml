            <dependency>
                <groupId>com.wrh</groupId>
                <artifactId>sublet-common-core</artifactId>
                <version>${project.version}</version>
            </dependency>
            <!--web模块-->
            <dependency>
                <groupId>com.wrh</groupId>
                <artifactId>sublet-common-web</artifactId>
                <version>${project.version}</version>
            </dependency>
            <!-- 用户管理api模块 -->
            <dependency>
                <groupId>com.wrh</groupId>
                <artifactId>sublet-user-api</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.wrh</groupId>
                <artifactId>sublet-post-api</artifactId>
                <version>${project.version}</version>
            </dependency>
